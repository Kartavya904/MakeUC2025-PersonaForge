# PersonaForge â€” Environment Template (public)
# Copy this file to `.env` (kept private via .gitignore) and fill in values.

# === Renderer URL ===
# This is the URL of the renderer process.
RENDERER_URL=http://localhost:5173

# === LLM / Planning ===
# Gemini 1.5 (planning to JSON plan)
GEMINI_API_KEY=

# === Speech ===
# ElevenLabs Text-to-Speech
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_ID=
ELEVENLABS_TTS_MODEL=eleven_multilingual_v2    
ELEVENLABS_STT_MODEL=scribe_v1  

# === App toggles ===
# 'true' to require manual approve step before running any action
APP_REQUIRE_APPROVAL=true
# 'info' | 'debug' | 'warn' | 'error'
LOG_LEVEL=info
